{
  "expo": {
    "name": "mapple",
    "slug": "mapple",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.mapple",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.mapple",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "mapple",
              "host": "reset-password",
              "pathPrefix": "/"
            }
          ],
          "category": ["DEFAULT", "BROWSABLE"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "mapple",
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
